{
    "routes": [
      {
        "route": "/logout",
        "serve": "/.auth/logout"
      },
      {
        "route": "/login",
        "serve": "/.auth/login/aad"
      },
      {
        "route": "/.auth/login/github",
        "statusCode": "404"
      },
      {
        "route": "/.auth/login/google",
        "statusCode": "404"
      },
      {
        "route": "/.auth/login/facebook",
        "statusCode": "404"
      },
      {
        "route": "/.auth/login/twitter",
        "statusCode": "404"
      },
      {
        "route": "/home",
        "serve": "/",
        "statusCode": 301
      },
      { 
        "route": "/search" ,
        "allowedRoles": ["supportteam"]
      },
      { 
        "route": "/api/search" ,
        "allowedRoles": ["supportteam"]
      },
      { 
        "route": "/api/lookup" ,
        "allowedRoles": ["supportteam"]
      },
      { 
        "route": "/api/suggest", 
        "allowedRoles": ["supportteam"]
      },
    
      {
        "route": "/*",
        "serve": "/",
        "statusCode": 200,
        "allowedRoles": ["supportteam"]
      }
    ],
    "platformErrorOverrides": [
      {
        "errorType": "Unauthenticated",
        "serve": "/login"
      },
      {
        "errorType": "NotFound",
        "serve": "/index.html",
        "statusCode": 200
      }
    ]
  }
